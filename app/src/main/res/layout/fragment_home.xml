<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:scrollbars="none">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/fragmentHomeRootLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="30dp">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/fragmentHomeRootLayoutGl97pv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.97" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/fragmentHomeRootLayoutGl67pv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.67" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/fragmentHomeRootLayoutGl65pv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.65" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/fragmentHomeRootLayoutGl35pv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.35" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/fragmentHomeRootLayoutGl33pv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.33" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/fragmentHomeRootLayoutGl3pv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.03" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/fragmentHomeRootLayoutGl50pv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_percent="0.5" />


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/fragmentHomeInternalBtn"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintDimensionRatio="w,1:1.5"
            app:layout_constraintEnd_toStartOf="@+id/fragmentHomeRootLayoutGl50pv"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/fragmentHomeExternalBtn"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintDimensionRatio="w,1:1.5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/fragmentHomeRootLayoutGl50pv"
            app:layout_constraintTop_toTopOf="parent">

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/fragmentHome1percentDeterminer"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/fragmentHomeRootLayoutGl35pv"
            app:layout_constraintStart_toStartOf="@+id/fragmentHomeRootLayoutGl33pv"
            app:layout_constraintTop_toTopOf="parent" />

        <include
            android:id="@+id/fragmentHomeAudiosBtn"
            layout="@layout/layout_home_tile"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintDimensionRatio="w,1.1:1"
            app:layout_constraintEnd_toStartOf="@+id/fragmentHomeRootLayoutGl33pv"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/fragmentHomeRootLayoutGl3pv"
            app:layout_constraintTop_toBottomOf="@+id/fragmentHomeInternalBtn" />

        <include
            android:id="@+id/fragmentHomeVideosBtn"
            layout="@layout/layout_home_tile"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintDimensionRatio="w,1.1:1"
            app:layout_constraintEnd_toStartOf="@+id/fragmentHomeRootLayoutGl65pv"
            app:layout_constraintStart_toStartOf="@+id/fragmentHomeRootLayoutGl35pv"
            app:layout_constraintTop_toBottomOf="@+id/fragmentHomeInternalBtn" />

        <include
            android:id="@+id/fragmentHomeDocsBtn"
            layout="@layout/layout_home_tile"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintDimensionRatio="w,1.1:1"
            app:layout_constraintEnd_toStartOf="@+id/fragmentHomeRootLayoutGl97pv"
            app:layout_constraintStart_toStartOf="@+id/fragmentHomeRootLayoutGl67pv"
            app:layout_constraintTop_toBottomOf="@+id/fragmentHomeInternalBtn" />

        <include
            android:id="@+id/fragmentHomeAppsBtn"
            layout="@layout/layout_home_tile"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintDimensionRatio="w,1.1:1"
            app:layout_constraintEnd_toStartOf="@+id/fragmentHomeRootLayoutGl33pv"
            app:layout_constraintStart_toStartOf="@+id/fragmentHomeRootLayoutGl3pv"
            app:layout_constraintTop_toBottomOf="@id/fragmentHomeAudiosBtn" />

        <include
            android:id="@+id/fragmentHomeImagesBtn"
            layout="@layout/layout_home_tile"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintDimensionRatio="w,1.1:1"
            app:layout_constraintEnd_toStartOf="@+id/fragmentHomeRootLayoutGl65pv"
            app:layout_constraintStart_toStartOf="@+id/fragmentHomeRootLayoutGl35pv"
            app:layout_constraintTop_toBottomOf="@+id/fragmentHomeVideosBtn" />

        <include
            android:id="@+id/fragmentHomeTransferPcBtn"
            layout="@layout/layout_home_tile"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintDimensionRatio="w,1.1:1"
            app:layout_constraintEnd_toStartOf="@id/fragmentHomeRootLayoutGl97pv"
            app:layout_constraintStart_toStartOf="@+id/fragmentHomeRootLayoutGl67pv"
            app:layout_constraintTop_toBottomOf="@+id/fragmentHomeDocsBtn" />

        <include
            android:id="@+id/fragmentHomeCloudBtn"
            layout="@layout/layout_home_tile"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintDimensionRatio="w,1.1:1"
            app:layout_constraintEnd_toStartOf="@+id/fragmentHomeRootLayoutGl33pv"
            app:layout_constraintStart_toStartOf="@+id/fragmentHomeRootLayoutGl3pv"
            app:layout_constraintTop_toBottomOf="@+id/fragmentHomeAppsBtn" />

        <include
            android:id="@+id/fragmentHomeTransferAppsBtn"
            layout="@layout/layout_home_tile"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintDimensionRatio="w,1.1:1"
            app:layout_constraintEnd_toStartOf="@+id/fragmentHomeRootLayoutGl65pv"
            app:layout_constraintStart_toStartOf="@+id/fragmentHomeRootLayoutGl35pv"
            app:layout_constraintTop_toBottomOf="@+id/fragmentHomeImagesBtn" />

        <include
            android:id="@+id/fragmentHomeNamelessBtn"
            layout="@layout/layout_home_tile"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintDimensionRatio="w,1.1:1"
            app:layout_constraintEnd_toStartOf="@id/fragmentHomeRootLayoutGl97pv"
            app:layout_constraintStart_toStartOf="@+id/fragmentHomeRootLayoutGl67pv"
            app:layout_constraintTop_toBottomOf="@+id/fragmentHomeTransferPcBtn" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>
